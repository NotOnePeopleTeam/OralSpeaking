/**app.wxss**/
.app {
  background-color: #f5f5f5;
  font-family:  Arial, Helvetica, sans-serif;
  color: #333;
}
.container {
  height: 100%;
  box-sizing: border-box;
  font-size: 14px;
} 
image{
  height: 100%;
  width: 100%;
  margin: 0;
  border: 0;
  padding: 0;
}
page{
  background-color: #f5f5f5;
  font-family: PingFang SC;
  color: #333333;
}
.appointment{
  background-color: #F44C70;
  color: white;
  border: solid 1px #F44C70;
  border-radius: 20px;
  text-align: center;
  height: 52rpx;
  width: 120rpx;
  font-size: 26rpx;
  line-height: 52rpx;
}
::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}
