.wrap{
    width: 100%;
    height: 100%;
    overflow: hidden;
    background-color: rgba(0, 0, 0, 0.43);
    position: absolute;
    top: 0;
    left: 0;
    display: flex;
    justify-content: center;
    text-align: center;
    align-items: center;
    flex-direction: column;
}
.content_select{
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
}

.select{
    display: flex;
    align-items: center;
    text-align: center;
    line-height: 60px;
}
.cancel_select{
    width: 50%;
}
.content_back{
    position: relative;
    width: 570rpx;
    height: 544rpx;
}
.line_mask{
    height: 1rpx;
    width: 427rpx;
    background-color: #D6D6D6;
    margin: 30rpx auto;
}
.confirm_only{
    width: 285rpx;
    height: 73rpx;
    line-height: 73rpx;
    background: #F44C70;
    box-shadow: 0px 6px 8px 5px rgba(244, 76, 112, 0.16);
    border-radius: 36rpx;
    margin: 40rpx auto;
    font-size: 30rpx;
    font-weight: bold;
    color: white;
    letter-spacing: 4rpx;
}
.close_icon{
    width: 50rpx;
    height: 50rpx;
    position: absolute;
    right: -25rpx;
    top: -25rpx;
}
.content_select .title_icon{
    width: 397rpx;
    height: 45rpx;
    margin: 116rpx auto 0;
}
.wechat_service,.wechat_phone{
    display: flex;
    flex-direction: row;
    justify-content: space-around;
    width: 427rpx;
    color: #131313;
    font-size: 28rpx;
    margin: 0 auto;
}
.wechat_service{
    margin-top: 110rpx;
}
.wechat_service image,.wechat_phone image{
    width: 30rpx;
    height: 30rpx;
}